{
  "name": "cradova",
  "version": "1.0.10",
  "description": "Web framework for building web apps and PWAs",
  "main": "dist/index",
  "source": "lib/index.ts",
  "type": "module",
  "isLibrary": true,
  "optimize": true,
  "targets": {
    "main": false
  },
  "files": [
    "dist/index.cjs",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FridayCandour/cradova.git"
  },
  "scripts": {
    "build": "tsup lib/index.ts && npm run ts",
    "ts": "cp cradova.d.ts dist/index.d.ts",
    "lint": "eslint . --fix"
  },
  "exports": {
    "default": "./dist/index.js",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "node": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "PWA",
    "framework",
    "frontend",
    "javascript",
    "typescript",
    "web",
    "app",
    "mobile",
    "android",
    "IOS",
    "windows",
    "linux",
    "mac"
  ],
  "author": "friday candour",
  "funding": [
    {
      "type": "patreon",
      "url": "https://www.patreon.com/FridayCandour"
    }
  ],
  "contributors": [
    {
      "name": "friday candour",
      "email": "FridayCandours@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FridayCandour/cradova/issues"
  },
  "homepage": "https://github.com/FridayCandour/cradova#readme",
  "displayName": "cradova web framework",
  "typings": "typescript",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
